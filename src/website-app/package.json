{
  "name": "website-app",
  "version": "0.0.1",
  "scripts": {
    "dev": "pnpm exec vite dev",
    "dev:expose": "pnpm exec vite dev --host",
    "build": "pnpm exec vite build",
    "package": "pnpm exec svelte package",
    "preview": "pnpm exec vite preview",
    "preview:expose": "pnpm exec vite preview --host",
    "test": "pnpm exec playwright test",
    "check": "pnpm exec svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "pnpm exec svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "pnpm exec prettier --check --plugin-search-dir=. . && eslint .",
    "format": "pnpm exec prettier --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@iconify/icons-akar-icons": "^1.2.15",
    "@iconify/svelte": "^3.1.1",
    "@playwright/test": "^1.32.3",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-node": "^1.2.3",
    "@sveltejs/kit": "^1.15.5",
    "@sveltejs/package": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "svelte-preprocess": "^5.0.3",
    "tailwindcss": "^3.3.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.2.1"
  },
  "type": "module",
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@tanstack/svelte-table": "^8.8.5",
    "daisyui": "^2.51.5",
    "pocketbase": "^0.13.1",
    "svelte-fa": "^3.0.3",
    "theme-change": "^2.5.0"
  }
}
